<div class="books-search-wrapper">
  <div class="container">
    <% @books.each do |book| %>
      <div class="row">
        <div class="col-md-8 offset-md-2">
          <%= link_to book_path(id: book.id) do %>
            <%= book.book_tittle %>
            <%= book.author %>
          <% end %>
        </div>
      </div>
    <% end %>
  </div>
</div>